<h1>Your Rounds</h1>
<%= link_to 'New Round', new_round_path %>

<table>
<thead>
  <tr>
    <th>Date</th>
    <th>Score</th>
    <th>Fairways</th>
    <th>Greens</th>
    <th>Putts</th>
    <th>Notes</th>
    <th>Edit</th>
  </tr>
</thead>

<tbody>
<% @rounds.each do |round| %>
  <tr>
    <td><%= link_to round.date_played, round %></td>
    <td><%= round.score %></td>
    <td><%= round.fairways %></td>
    <td><%= round.greens %></td>
    <td><%= round.putts %></td>
    <td><%= round.notes %></td>
    <td><%= link_to 'Edit', edit_round_path(round) %></td>
  </tr>
<% end %>
</tbody>
</table>